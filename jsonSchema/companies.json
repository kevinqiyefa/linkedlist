{
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "The Name Schema ",
      "default": "",
      "examples": ["Facebook"],
      "minLength": 1,
      "maxLength": 55
    },
    "logo": {
      "$id": "/properties/logo",
      "type": "string",
      "title": "The Logo Schema ",
      "default": "",
      "format": "uri",
      "examples": [
        "https://cdn.pixabay.com/photo/2017/10/04/11/58/facebook-2815970_960_720.jpg"
      ]
    },
    "handle": {
      "$id": "/properties/handle",
      "type": "string",
      "title": "The Handle Schema ",
      "default": "",
      "examples": ["test3"],
      "minLength": 1,
      "maxLength": 55
    },
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",
      "default": "",
      "examples": ["12345"],
      "minLength": 1,
      "maxLength": 55
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "title": "The Email Schema ",
      "default": "",
      "format": "email",
      "examples": ["fb@gmail.com"],
      "minLength": 1,
      "maxLength": 55
    }
  },
  "required": ["name", "handle", "password", "email"]
}
