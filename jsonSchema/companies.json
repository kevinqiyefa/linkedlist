{
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "The Name Schema ",
      "default": "",
      "examples": ["Facebook"],
      "minLength": 3
    },
    "logo": {
      "$id": "/properties/logo",
      "type": "string",
      "title": "The Logo Schema ",
      "default": "",
      "examples": [
        "https://cdn.pixabay.com/photo/2017/10/04/11/58/facebook-2815970_960_720.jpg"
      ]
    },
    "handle": {
      "$id": "/properties/handle",
      "type": "string",
      "title": "The Handle Schema ",
      "default": "",
      "examples": ["test"],
      "minLength": 5
    },
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",
      "default": "",
      "examples": ["12345"],
      "minLength": 5
    }
  },
  "required": ["name", "handle", "password"]
}
