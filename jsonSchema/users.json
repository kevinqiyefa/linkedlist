{
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "first_name": {
      "$id": "/properties/first_name",
      "type": "string",
      "title": "The First_name Schema ",
      "default": "",
      "examples": ["Carrie"],
      "minLength": 1,
      "maxLength": 55
    },
    "last_name": {
      "$id": "/properties/last_name",
      "type": "string",
      "title": "The Last_name Schema ",
      "default": "",
      "examples": ["Lu"],
      "minLength": 1,
      "maxLength": 55
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "title": "The Email Schema ",
      "default": "",
      "examples": ["test@gmail.com"],
      "minLength": 1,
      "maxLength": 55
    },
    "photo": {
      "$id": "/properties/photo",
      "type": "string",
      "title": "The Photo Schema ",
      "default": "",
      "examples": ["https://i.imgur.com/gdWIxn2.jpg"]
    },
    "current_company": {
      "$id": "/properties/current_company",
      "type": "integer",
      "title": "The Current_company Schema ",
      "default": 0,
      "examples": [1]
    },
    "username": {
      "$id": "/properties/username",
      "type": "string",
      "title": "The Username Schema ",
      "default": "",
      "examples": ["carrie"],
      "minLength": 1,
      "maxLength": 55
    },
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",
      "default": "",
      "examples": ["test"],
      "minLength": 1,
      "maxLength": 55
    }
  },
  "required": ["first_name", "last_name", "email", "username", "password"]
}
